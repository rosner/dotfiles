[user]
	name = Norman Rosner
	email = norman@rosner.io
[alias]
	co = checkout
[color]
	diff = auto
	status = auto
	branch = auto
[core]
	excludesfile = <%= ENV['HOME'] %>/.gitignore
	autocrlf = input
[apply]
	whitespace = nowarn
[format]
	pretty = %C(yellow)%h%Creset %s %C(red)(%an, %cr)%Creset
[github]
	user = rosner
	token = <%= print("GitHub API Token: "); STDOUT.flush; STDIN.gets.chomp %>
